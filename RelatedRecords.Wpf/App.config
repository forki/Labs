            <string>100</string>
            <string>500</string>
            <string>1000</string>
            <string>5000</string>
            <string>10000</string>
            <string>50000</string>
          </ArrayOfString>
        </value>
      </setting>
      <setting name="UseSchemaConstraints" serializeAs="String">
        <value>False</value>
      </setting>
    </RelatedRecords.Wpf.Properties.Settings>
  </userSettings>
</configuration>